{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "deploy": "SKIP_PREFLIGHT_CHECK=true NODE_ENV=production yarn build && yarn deploy:netlify",
    "deploy:netlify": "netlify deploy --dir ./build --prod --functions ./noop"
  },
  "dependencies": {
    "@1productaweek/react-modal-hooks": "^0.1.14",
    "@chakra-ui/anatomy": "^2.1.0",
    "@chakra-ui/react": "^2.4.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/inter": "^4.5.14",
    "@polybase/client": "^0.3.10",
    "@polybase/eth": "^0.3.9",
    "@polybase/polylang": "^0.3.1",
    "@polybase/react": "^0.3.6",
    "@polybase/util": "^0.3.10",
    "@sentry/react": "^7.26.0",
    "@sentry/tracing": "^7.26.0",
    "@stripe/react-stripe-js": "^1.16.1",
    "@stripe/stripe-js": "^1.46.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "axios": "^0.27.2",
    "framer-motion": "^7.10.0",
    "history": "^5.3.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "posthog-js": "^1.38.1",
    "react": "^18.1.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.1.0",
    "react-flip-numbers": "^3.0.7",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.40.0",
    "react-icons": "^4.7.1",
    "react-infinite-scroller": "^1.2.6",
    "react-loadable": "^5.5.0",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^6.4.5",
    "react-scripts": "5.0.1",
    "react-table": "^7.8.0",
    "typescript": "^4.9.4",
    "use-interval": "^1.4.0",
    "web-vitals": "^3.1.0",
    "web3": "^1.8.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": [
        2,
        {
          "skipUndeclared": true
        }
      ],
      "react/jsx-handler-names": "off",
      "quotes": "off",
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "react/display-name": 0,
      "semi": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "arrow-body-style": 0,
      "no-use-before-define": 0,
      "arrow-parens": 0,
      "no-trailing-spaces": "error",
      "@typescript-eslint/quotes": [
        "error",
        "single"
      ],
      "@typescript-eslint/indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/react-helmet": "^6.1.5",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-loadable": "^5.5.6",
    "@types/react-resizable": "^3.0.3",
    "@types/react-table": "^7.7.12",
    "add": "^2.0.6",
    "assert": "^2.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-textarea-autosize": "^8.4.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "yarn": "^1.22.19"
  }
}
